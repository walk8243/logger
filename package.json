{
  "name": "@walk8243/logger",
  "version": "2.3.0",
  "description": "log4jsの設定にデフォルトを設ける",
  "main": "lib/logger.js",
  "scripts": {
    "build": "yarn run tsc --sourceMap false --removeComments true",
    "build-debug": "yarn run tsc --build",
    "clean": "yarn run tsc --build --clean",
    "cover": "yarn run test && yarn run nyc report --reporter=html --reporter=text",
    "init": "yarn && yarn run build-debug",
    "mocha": "yarn run nyc mocha test/**/*.ts",
    "node": "node index",
    "package": "yarn && yarn run test && yarn run build && npm publish",
    "start": "yarn run build && yarn run node",
    "test": "yarn run clean && yarn run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/walk8243/logger.git"
  },
  "keywords": [
    "walk8243",
    "logger",
    "log4js"
  ],
  "author": "walk8243",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/walk8243/logger/issues"
  },
  "homepage": "https://github.com/walk8243/logger#readme",
  "dependencies": {
    "log4js": "^6.3.0",
    "rfdc": "^1.1.4"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.16",
    "@types/sinon": "^9.0.10",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "sinon": "^9.2.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "directories": {
    "test": "test"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "**/*.ts"
    ],
    "exclude": [
      "*.d.ts",
      "test/**/*.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "none"
    ]
  }
}
